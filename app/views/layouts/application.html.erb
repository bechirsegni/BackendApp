<!DOCTYPE html>
<html>
  <head>
    <title>BackendApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
     <% if user_signed_in? %>
     <%= current_user.email%>
      <%= link_to 'Sign out', destroy_user_session_path, method: 'delete' %>
     <%end%>
    <%= yield %>
  </body>
</html>
